{
  "printWidth":80,
  "tabWidth":2,
  "useTabs":false,
  "semi":true,
  "singleQuote":true,
  "trailingComma":"all",
  "bracketSpacing":true,
  "arrowParens":"always",
  "plugins": ["prettier-plugin-curly", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-redux$",
    "^redux$",
    "^reselect$",
    "",
    "^@mui/(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@api/(.*)$",
    "",
    "^@store/(.*)$",
    "",
    "^@helpers/(.*)$",
    "",
    "^@formatters/(.*)$",
    "",
    "^@core/(.*)$",
    "",
    "^@components/(.*)$",
    "",
    "^app/(.*)$",
    "",
    "^[./]"]
}
